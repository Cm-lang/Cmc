
Expr    ::= Or+
Or      ::= AtomExpr ('|' AtomExpr)* 

AtomExpr::= Atom Trailer 
Atom    ::= Name | Str | '[' Expr ']' | '(' Expr ')' 


Def    ::= '::='
Equals ::= Name Def Expr

Trailer::= ['*' | '+' | '{' Number{1 2} '}']

Name   ::= R'[a-zA-Z_][a-zA-Z0-9]*'
Str    ::= R'[\w|\W]*'
Number ::= R'\d+'




